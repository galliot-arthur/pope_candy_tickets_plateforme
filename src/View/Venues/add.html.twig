{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Ajoutez une salle de concert</h1>
    <form method="post">
    <input type="hidden" value="{{ venue.id }}" name="id">
    <label for="town" > Ville </label>
    <input type="text" value="{{ venue.town }}" name="town">
    <input type="text" value="{{ venue.disabled_acces }}" name="disabled_acces">
    <input type="text" value="{{ venue.adress }}" name="adress">
    <input type="text" value="{{ venue.capacity }}" name="capacity">
    <input type="text" value="{{ venue.vip_available }}" name="vip_available">
    <input type="text" value="{{ venue.prices }}" name="prices">
    <input type="submit" value="Add">
</form>
<a href="/item/index">back to list</a>
{% endblock %}
